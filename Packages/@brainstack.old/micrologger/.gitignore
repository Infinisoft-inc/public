# .gitignore

# Node modules and dependencies
node_modules/

# Coverage directory used by tools like Istanbul
coverage/

# Build artifacts and distribution files
dist/
build/

# Log files
*.log

# IDE and editor directories and files
.vscode/
.idea/
*.sublime*
*.iml

# Temporary files and directories
*.tmp
*.swp
*.swo
*.bak
.DS_Store
Thumbs.db

# Environment variables and secrets
.env
.env.local
.env.*.local

# Ignore npm debug log
npm-debug.log

# Ignore yarn integrity file
.yarn-integrity
