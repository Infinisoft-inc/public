{
  "name": "@brainstack/auth",
  "version": "1.0.8",
  "description": "The Brainstack Auth SDK is a powerful tool for managing user authentication in various applications. It provides a unified interface to interact with authentication methods, abstracting the underlying complexities and offering a streamlined API.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "authentication",
    "user authentication",
    "auth SDK",
    "security"
  ],
  "author": "Infini SoftWorld <info@infinisoft.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Infinisoft-inc/public.git"
  },
  "bugs": {
    "url": "https://github.com/Infinisoft-inc/public/issues"
  },
  "homepage": "https://github.com/Infinisoft-inc/public#readme",
  "scripts": {
    "test": "set NODE_OPTIONS=--experimental-vm-modules && jest --coverage index.test.ts",
    "test:dev": "jest --watchAll",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/index.js",
    "types": "tsc --declaration --outDir ../typings/auth --emitDeclarationOnly"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.7.10",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.4.2"
  }
}
